{
    "name": "misterabdul/doctrine-data-fixture-module",
    "description": "Laminas Module that provides Doctrine Data-Fixture functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "php8",
        "laminas"
    ],
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        },
        {
            "name": "Dolly Aswin",
            "email": "dolly.aswin@gmail.com"
        },
        {
            "name": "Abdul Pasaribu",
            "email": "abdoelrachmad@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "laminas/laminas-modulemanager": "^2.11.0",
        "laminas/laminas-eventmanager": "^3.5.0",
        "laminas/laminas-servicemanager": "^3.14.0",
        "doctrine/data-fixtures": "^1.5.3",
        "doctrine/doctrine-orm-module": "^5.2.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.5.2",
        "phpunit/phpunit": "^9.5.21",
        "squizlabs/php_codesniffer": "^3.7.1"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "DoctrineDataFixtureModule\\": "src/DoctrineDataFixtureModule/"
        }
    }
}
